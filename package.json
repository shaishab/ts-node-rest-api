{
  "name": "ts-node-rest-api",
  "version": "1.0.0",
  "description": "This is the simple example or starter to create rest API using express with typescript.",
  "main": "server.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development node ./dist/server.js",
    "start:test": "cross-env NODE_ENV=test node ./dist/server.js",
    "start:prod": "cross-env NODE_ENV=productino node ./dist/server.js",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "build": "rm -rf ./dist/ && tsc",
    "build:dev": "npm run build && npm run start:dev",
    "build:test": "npm run build && npm run start:test",
    "build:prod": "npm run build && npm run start:prod"
  },
  "keywords": [
    "nodejs",
    "node.js",
    "express",
    "typescript",
    "mongodb",
    "strater",
    "boilerplate",
    "example"
  ],
  "author": "shaishab.cse@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/glob": "^7.1.1",
    "@types/helmet": "0.0.44",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/lodash": "^4.14.138",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^12.7.4",
    "@types/uuid": "^3.4.5",
    "cross-env": "^5.2.1",
    "ts-node-dev": "^1.0.0-pre.42",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "helmet": "^3.21.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mongoose": "^5.7.5",
    "uuid": "^3.3.3"
  }
}
